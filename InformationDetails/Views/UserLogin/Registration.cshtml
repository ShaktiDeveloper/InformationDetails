    <div class="login-10">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-4 col-lg-5 col-md-12 col-pad-0 bg-color-10">
                    <div class="form-section">
                        <div class="logo">
                            <a href="#">
                                <img src="~/assets/img/logos/logo.png" alt="logo">
                            </a>
                        </div>
                        <div class="btn-section">
                          
                                @*<a href="login-10.html" class="link-btn">Login</a>*@
                                 
                            @*<a href="login-10.html" class="link-btn">Login</a>*@
                            <input type="button" class="link-btn" value="Login" onclick="location.href='@Url.Action("Login", "UserLogin")'" />

                            <input type="button" class="btn btn-info" value="AddRole" onclick="location.href='@Url.Action("AddRole", "UserLogin")'" />

                        </div>
                        @using (Html.BeginForm("Registration", "UserLogin", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Raw(ViewBag.CustomerName)
                            <div class="login-inner-form">

                                <form action="http://storage.googleapis.com/themevessel-products/logdy/index.html" method="GET">
                                    <div class="form-group form-box">
                                        <input type="text" id="fullname" name="fullname" class="input-text" placeholder="Full Name">
                                        @*@Html.ValidationMessage("fullname", "", new { @class = "text-danger" })*@
                                        <span class="error">This field is required</span>
                                        <i class="flaticon-user"></i>
                                    </div>
                                    <div class="form-group form-box">
                                        <input type="email" id="EmailAddress" name="EmailAddress" class="input-text" placeholder="Email Address">
                                        @Html.ValidationMessage("EmailAddress", "", new { @class = "text-danger" })
                                        <i class="flaticon-mail-2"></i>
                                    </div>
                                    <div class="form-group form-box">
                                        <input type="password" id="Password" name="Password" class="input-text" placeholder="Password">

                                        @Html.ValidationMessage("Password", "", new { @class = "text-danger" })
                                        <i class="flaticon-password"></i>
                                    </div>
                                    <div class="form-group form-box">
                                        @Html.DropDownList("Name", new SelectList(ViewBag.Name, "Id", "RoleName"), "       --Select Role--     ", new { @onChange = "selectindex(this)", @class = "form-control", @placeholder = "-select-" })

                                        @*@Html.DropDownList("Name", (IEnumerable<SelectListItem>)ViewBag.Name, new { id = "ddlLocations", @class = "form-control" })*@

                                        @Html.ValidationMessage("RoleName", "", new { @class = "text-danger" })
                                        @*<i class="flaticon-password"></i>*@
                                    </div>
                                    <div class="form-group form-box">
                                        <input type="hidden" name="RoleId" readonly="readonly" id="role" class="input-text" placeholder="Role">
                                        @Html.ValidationMessage("Role", "", new { @class = "text-danger" })
                                    </div>
                                    <div class="checkbox clearfix">
                                        <div class="form-check checkbox-theme">
                                            <input class="form-check-input" type="checkbox" value="" id="rememberMe">
                                            <label class="form-check-label" for="rememberMe">
                                                I agree to the<a href="#" class="terms">terms of service</a>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="form-group mb-0">
                                        <button type='submit' value='Submit' @*onclick='return validate();'*@ class="btn-md btn-theme btn-block">Register</button>
                                    </div>
                                </form>
                                <div class="extra-login">
                                    <span>Or Login With</span>
                                </div>
                                <ul class="social-list clearfix">
                                    <li><a href="#" class="facebook-bg">Facebook</a></li>
                                    <li><a href="#" class="twitter-bg">Twitter</a></li>
                                    <li><a href="#" class="google-bg">Google</a></li>
                                </ul>
                            </div>
                        }
